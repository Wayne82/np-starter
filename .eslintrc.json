{
    "env": {
        "browser": true,
        "es2021": true,
        "jasmine": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["jasmine"],
    "ignorePatterns": ["dist"],
    "rules": {
        "prettier/prettier": ["error", {"singleQuote": true}]
    }
}
